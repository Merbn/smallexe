.container{
  background-color: #b3d4db;
  display: flex;
  flex-direction: column; 
  align-items: center;
  height:1200rpx;
  width: 750rpx;
}
.user-avatar{
  width: 200rpx;
  height: 200rpx;
  margin-top: 160rpx;
}
.user-name{
  margin-top: 100rpx;
  font-size: 32rpx;
  font-weight: bold;
} 
.moto{
  font-size: 22rpx;
  font-weight: bold;
  line-height: 80rpx;
  color: #ff0;
}
.moto-container{
  margin-top: 200rpx;
  border: 1px solid #ff0;
  width: 200rpx;
  height: 80rpx;
  border-radius: 5px;
  text-align: center;

}
.page{
  height: 100%;
  background-color: #b3d4db;
}
.swiper {
  width: 700rpx;
  height: 700rpx;
}

.swiper images {
  width: 700rpx;
  height: 600rpx;
}
